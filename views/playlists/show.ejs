<%- include('../partials/header') %>

<table>
  <thead>
    <tr>
      <th>Song</th>
      <th>Artist</th>
    </tr>
  </thead>
  <tbody>
      <% playlist.songs.forEach(function(s) { %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.artist %></td>
      </tr>
      <% }) %>  
    </tbody>
  </table>

<%- include('../partials/footer') %>